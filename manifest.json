{
  "manifest_version": 2,
  "name": "Fallen London Storylets Pin",
  "version": "1.0",
  "description": "Adds the ability to pin storylets in fallen london.",
  "content_scripts": [
    {
      "matches": [
        "*://*.fallenlondon.com/*"
      ]
    }
  ],
  "background": {
    "scripts": [
      "pin.js"
    ]
  },
  "permissions": [
    "storage",
    "webRequest"
  ]
}